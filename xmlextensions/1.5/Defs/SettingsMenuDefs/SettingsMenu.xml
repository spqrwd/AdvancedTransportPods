      <li Class="XmlExtensions.Setting.SetColor">
        <color>(255,50,50)</color>
      </li>
      <li Class="XmlExtensions.Setting.Text">
        <text>For any changes to take effect the game must be restarted!</text>
        <font>Medium</font>
        <anchor>Middle</anchor>
        <tKey>AdvancedTransportPodsWarning</tKey>
      </li>
      <li Class="XmlExtensions.Setting.SetColor"/>
      <li Class="XmlExtensions.Setting.GapLine"/>

      <!-- Pod Launchers -->

      <li Class="XmlExtensions.Setting.Text">
        <text>Settings for Pod Launchers</text>
        <font>Medium</font>
        <anchor>Middle</anchor>
        <tKey>PodLaunchersSettingsTitle</tKey>
      </li>
      <li Class="XmlExtensions.Setting.GapLine"/>

      <!-- Vanilla Pod Launcher -->

      <li Class="XmlExtensions.Setting.Checkbox">
        <label>Vanilla Pod Launcher Custom Settings</label>
        <key>VanillaPodLauncherSettingsOn</key>
        <defaultValue>False</defaultValue>
        <tKey>VanillaPodLauncherSettingsTitle</tKey>
      </li>
      <li Class="XmlExtensions.Setting.ToggleableSettings">
        <key>VanillaPodLauncherSettingsOn</key>

        <caseFalse>
          <li Class="XmlExtensions.Setting.SetColor">
            <color>(163,177,188)</color>
          </li>
          <li Class="XmlExtensions.Setting.Text">
            <text>This setting does not disable the vanilla Pod Launcher, but enables you to change some of its attributes.</text>
            <tKey>VanillaPodLauncherSettingsOnDesc</tKey>
          </li>
          <li Class="XmlExtensions.Setting.SetColor"/>
        </caseFalse>

        <caseTrue>
          <li Class="XmlExtensions.Setting.SetColor">
            <color>(163,177,188)</color>
          </li>
          <li Class="XmlExtensions.Setting.Slider">
            <key>vanillaPodLauncherFuelCapacity</key>
            <defaultValue>150</defaultValue>
            <min>1</min>
            <max>300</max>
            <decimals>0</decimals>
            <label>Maximum fuel capacity: {vanillaPodLauncherFuelCapacity}  (default: 150)</label>
            <tKey>VanillaPodLauncherSettingsFuelCapacity</tKey>
          </li>
          <li Class="XmlExtensions.Setting.SetColor"/>
          <li Class="XmlExtensions.Setting.SetColor">
            <color>(163,177,188)</color>
          </li>
          <li Class="XmlExtensions.Setting.Text">
            <text>Adjust the maximum fuel capacity for the Vanilla Pod Launcher.</text>
            <tKey>VanillaPodLauncherSettingsFuelCapacityDesc</tKey>
